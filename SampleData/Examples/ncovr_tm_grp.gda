                <path>ncovr/ncovr.sqlite</path>
            </datasource>
            <layername>ncovr</layername>
            <title>NCOVR 1960-90</title>
            <variable_order>
                <time_ids>
                    <id>1960</id>
                    <id>1970</id>
                    <id>1980</id>
