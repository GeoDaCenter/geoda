                    <var>UE60</var>
                    <var>UE70</var>
                    <var>UE80</var>
                    <var>UE90</var>
                </group>
                <group>
                    <name>Homicide Count</name>
                    <displayed_decimals>4</displayed_decimals>
                    <var>HC60</var>
                    <var>HC70</var>
                    <var>HC80</var>
                    <var>HC90</var>
                </group>
                <group>
                    <name>Resource Depr.</name>
